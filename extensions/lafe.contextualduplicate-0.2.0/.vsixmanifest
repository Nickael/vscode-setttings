        <Property Id="Microsoft.VisualStudio.Services.Links.Repository" Value="https://github.com/lafe/ContextualDuplicate.git" />
      
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://github.com/lafe/ContextualDuplicate/issues" />
