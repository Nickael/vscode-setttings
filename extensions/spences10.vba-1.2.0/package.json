{
	"name": "vba",
	"displayName": "VSCode VBA",
	"description": "VBA language syntax highlighting support and code snippets for VSCode",
	"version": "1.2.0",
	"icon": "images/logo.png",
	"publisher": "spences10",
	"engines": {
		"vscode": "^1.8.1"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/spences10/vscode-vba.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "vba",
				"aliases": [
					"VBA",
					"vba"
				],
				"extensions": [
					".bas",
					".cls",
					".frm",
					".sht",
					".wbk"
				],
				"configuration": "./vba.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "vba",
				"scopeName": "source.vba",
				"path": "./syntaxes/vba.json"
			}
		],
		"snippets": [
			{
				"language": "vba",
				"path": "./snippets/vba.json"
			}
		]
	},
	"__metadata": {
		"id": "027f1577-c3da-4692-87d0-ba6a6d40757c",
		"publisherId": "c2f4e1c5-4505-4f79-be50-cc7fac10417b",
		"publisherDisplayName": "Scott Spence"
	}
}