        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.vscode": true,
        "**/Ma musique": true,
        "**/Mes images": true,
        "**/Mes vidéos": true,
        "desktop.ini": true,
        "**/.config": true,
        "**/.PhpStorm2019.1": true,
        "**/.symfony": true,
        "**/.vagrant.d": true,
        "**/3D Objects": true,
        "**/AppData": true,
        "**/NTUSER.DAT": true,
        "**/*.dat.*": true,
        "**/*.blf": true,
        "**/*.pol": true,
        "**/*.regtrans-ms": true,
        "**/Contacts": true,
        "**/Cookies": true,
        "**/Favorites": true,
        "**/Application Data": true,
        "**/IntelGraphicsProfiles": true,
        "**/Links": true,
        "**/Menu Démarrer": true,
