{
    "clock.dateFormat": "dd.mm.yyyy -  HH:MM:ss",
    "editor.fontFamily": "Hasklig",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "boundary",
    "editor.scrollBeyondLastLine": false,
    "files.associations": {
        "*.cnf": "apacheconf",
        "*.conf": "apacheconf",
        "*.pagets": "typoscript",
        "*.setupts": "typoscript",
        "*.php*": "php",
        "*rc": "shellscript",
        "AdditionalConfiguration.php*": "php",
        "aliases": "yaml",
        "constants.ts": "typoscript",
        "constants.txt": "typoscript",
        "crontab": "shellscript",
        "Default.ts": "typoscript",
        "DIR_COLORS": "shellscript",
        "hosts.ics": "shellscript",
        "hosts": "shellscript",
        "LocalConfiguration.php*": "php",
        "networks": "shellscript",
        "profile": "shellscript",
        "protocols": "shellscript",
        "realurl_conf.php*": "php",
        "services": "shellscript",
        "setup.ts": "typoscript",
        "setup.txt": "typoscript",
        "tsconfig.txt": "typoscript"
    },
    "files.exclude": {
        // "**/_**": true,
        "**/.config": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.idea": true,
        "**/.PhpStorm2019.1": true,
        "**/.svn": true,
        "**/.symfony": true,
        "**/.vagrant.d": true,
        // "**/.vscode": true,
        "**/*.blf": true,
        "**/*.dat.*": true,
        "**/*.dat": true,
        "**/*.exe": true,
        "**/*.msg": true,
        "**/*.pol": true,
        "**/*.regtrans-ms": true,
        "**/3D Objects": true,
        "**/AppData": true,
        "**/Application Data": true,
        "**/cache_core": true,
        "**/cache_phpcode": true,
        "**/Contacts": true,
        "**/Cookies": true,
        "**/Creative Cloud Files": true,
        "**/CVS": true,
        "**/Favorites": true,
        // "**/fileadmin": true,
        "**/fluid_template": true,
        "**/IntelGraphicsProfiles": true,
        "**/Links": true,
        "**/Ma musique": true,
        "**/Menu Démarrer": true,
        "**/Mes images": true,
        "**/Mes vidéos": true,
        "**/Modèles": true,
        "**/NTUSER.DAT": true,
        "**/Recent": true,
        "**/Roaming": true,
        "**/Saved Games": true,
        "**/Searches": true,
        "**/SendTo": true,
        "**/typo3temp": true,
        "**/uploads": true,
        "**/Videos": true,
        "**/Voisinage d'impression": true,
        "**/Voisinage réseau": true,
        "desktop.ini": true
    },
    "guides.active.color.dark": "#FFFFFE",
    "guides.active.color.light": "#52325d",
    "linesinfostatusbar.selectedDisplayFormat": "- %d Selected",
    "linesinfostatusbar.totalDisplayFormat": "Total : %d ",
    "projectNameInStatusBar.align": "left",
    "projectNameInStatusBar.alignPriority": 1000,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "window.newWindowDimensions": "maximized",
    "window.title": "${activeEditorShort} ${dirty}${separator}",
    "workbench.colorCustomizations": {
        "activityBar.background": "#FFFFFE",
        "activityBar.foreground": "#52325d",
        "activityBarBadge.background": "#52325d",
        "button.background": "#52325d",
        "editor.lineHighlightBackground": "#8080800e",
        "editorGroupHeader.noTabsBackground": "#FFFFFE",
        "editorGroupHeader.tabsBackground": "#FFFFFE",
        "editorGutter.background": "#F3F3F3",
        "editorLineNumber.activeForeground": "#52325d",
        "editorLineNumber.foreground": "#673F74",
        "extensionButton.prominentBackground": "#52325d",
        "extensionButton.prominentHoverBackground": "#52325d",
        "focusBorder": "#F3F3F3",
        "input.background": "#F3F3F3",
        "inputOption.activeBorder": "#F3F3F3",
        "list.activeSelectionBackground": "#F3F3F3",
        "list.activeSelectionForeground": "#313131",
        "list.hoverBackground": "#F3F3F3",
        "list.inactiveFocusBackground": "#673F74",
        "list.inactiveSelectionBackground": "#F3F3F3",
        "list.inactiveSelectionForeground": "#313131",
        "menu.selectionBackground": "#52325d",
        "menu.selectionForeground": "#FFFFFE",
        "panel.background": "#F3F3F3",
        "panel.border": "#F3F3F3",
        "panelTitle.activeBorder": "#52325d",
        "panelTitle.activeForeground": "#52325d",
        "sideBar.background": "#FFFFFE",
        "sideBarSectionHeader.background": "#EAE8EA",
        "sideBarSectionHeader.border": "#EAE8EA",
        "sideBarSectionHeader.foreground": "#52325d",
        "statusBar.background": "#F3F3F3",
        "statusBar.foreground": "#52325d",
        "statusBar.noFolderBackground": "#F3F3F3",
        "statusBar.noFolderForeground": "#52325d",
        "tab.activeBackground": "#FFFFFE",
        "tab.activeBorder": "#52325d",
        "tab.border": "#FFFFFE",
        "tab.hoverBorder": "#673F74",
        "tab.inactiveBackground": "#FFFFFE",
        "titleBar.activeBackground": "#52325d",
        "titleBar.activeForeground": "#fff",
        "titleBar.inactiveBackground": "#673F74"
    },
    "workbench.colorTheme": "FlatUI",
    "workbench.editor.tabCloseButton": "off",
    "workbench.iconTheme": "vs-nomo-dark",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "extensions.ignoreRecommendations": true
}
